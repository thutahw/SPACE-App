{
  "name": "space-app",
  "version": "1.0.0",
  "private": true,


  "workspaces": [          
    "apps/api",
    "apps/web"
  ],

  "packageManager": "npm@10.5.0",

  "scripts": {
    "dev":   "concurrently \"npm run dev   -w api\" \"npm start -w web\"",
    "start": "concurrently \"npm start -w api\"     \"npm start -w web\"",
    "build": "npx turbo run build"     
  },

  "devDependencies": {
    "concurrently": "^8.0.1",
    "turbo": "^2.5.3"
  },

  "dependencies": {
    "express": "^5.1.0",
    "multer":  "^2.0.0"
  }
}